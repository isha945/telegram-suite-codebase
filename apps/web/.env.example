# Environment Variables
# Copy this file to .env and fill in the values

# Name of the AI agent (required)
AGENT_NAME=MyAgent

# OpenRouter API key for LLM access (required) [secret]
OPENROUTER_API_KEY=

# Model to use via OpenRouter
OPENROUTER_MODEL=openai/gpt-4o

# ERC-8004 registry contract address
NEXT_PUBLIC_AGENT_REGISTRY_ADDRESS=

# Agent wallet private key for registry operations [secret]
AGENT_PRIVATE_KEY=

# Network for agent operations (arbitrum or arbitrum-sepolia) (required)
NEXT_PUBLIC_AGENT_NETWORK=arbitrum

# Ethereum address to receive payments (required)
PAYMENT_RECEIVER_ADDRESS=

# Private key for signing receipts (required) [secret]
PAYMENT_PRIVATE_KEY=

# Arbitrum RPC URL for deployment (required)
STYLUS_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Private key for deployment (required) [secret]
DEPLOYER_PRIVATE_KEY=

# Bot token from @BotFather (required) [secret]
TELEGRAM_BOT_TOKEN=

# API key for OpenAI (required) [secret]
OPENAI_API_KEY=

# Secret for webhook verification (required) [secret]
TELEGRAM_WEBHOOK_SECRET=

# WalletConnect Cloud project ID (required)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Application name for wallet dialogs
NEXT_PUBLIC_APP_NAME=My DApp

# Your bot username (without @) (required)
NEXT_PUBLIC_TELEGRAM_BOT_USERNAME=